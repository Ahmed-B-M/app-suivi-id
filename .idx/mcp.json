{
  "mcpServers": {
    "firebase": {
      "command": "npx",
      "args": [
        "-y",
        "firebase-tools@latest",
        "experimental:mcp",
        "--only",
        "auth,firestore"
      ]
    },
    "urbantz": {
      "command": "node",
      "args": [
        "./scripts/urbantz-mcp-server.js"
      ],
      "env": {
        "PORT": "8081"
      }
    }
  }
}
