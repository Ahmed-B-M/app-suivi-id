{
    "editor.experimentalGpuAcceleration": "on",
    "terminal.integrated.gpuAcceleration": "on",
    "files.participants.timeout": 600000,
    "diffEditor.maxComputationTime": 600000,
    "http.proxySupport": "off",
    "workbench.startupEditor": "none",
    "search.exclude": {
        "**/node_modules": true,
        "**/.next": true
    },
    "files.watcherExclude": {
        "**/node_modules/**": true,
        "**/.next/**": true
    },
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },
    "breadcrumbs.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit",
        "source.fixAll.eslint": "explicit"
    },
    "editor.experimental.treeSitterTelemetry": false,
    "diffEditor.maxFileSize": 90,
    "workbench.commandPalette.experimental.suggestCommands": false,
    "workbench.colorTheme": "Google Light",
    "search.actionsPosition": "auto",
    "search.collapseResults": "auto",
    "search.maxResults": 200000,
    "debug.allowBreakpointsEverywhere": true,
    "terminal.integrated.suggest.enabled": true,
    "json.schemas": []
}